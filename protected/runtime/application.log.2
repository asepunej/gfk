              group by ta.id_barang)t2 on t1.id_barang=t2.id_barang
              
LEFT JOIN (SELECT ta.id_barang,
                sum(ta.volume)as volume,
                sum(ta.jumlah) as jumlah  
              FROM 
                master.penerimaan_detail ta
              LEFT JOIN master.penerimaan tb on ta.id_penerimaan=tb.id_penerimaan
              where tb.id_distributor !='4' and tb.id_skpd=_id_skpd
              group by ta.id_barang)t3 on t1.id_barang=t3.id_barang



LEFT JOIN (  select t3.id_barang,sum(t1.volume),
            CAST(sum(t1.volume*t3.harga_satuan) as DOUBLE PRECISION) as jumlah
             from master.pengeluaran_detail t1
            left join master.pengeluaran t2 on t1.id_pengeluaran=t2.id_pengeluaran
            left join master.penerimaan_detail t3 on t1.id_penerimaan_detail=t3.id_penerimaan_detail
            where t2.id_skpd=_id_skpd
            group by t3.id_barang
             )t4 on t1.id_barang=t4.id_barang

LEFT JOIN referensi.jenis_barang t5 on  t1.id_jenis_barang=t5.id_jenis_barang
LEFT JOIN referensi.jenis_persediaan t6 on t5.id_jenis_persediaan=t6.id_jenis_persediaan

WHERE COALESCE(t2.volume,0)>0 
or COALESCE(t3.volume,0)>0
or COALESCE(t4.volume,0)>0
--or  COALESCE(t2.volume,0)+COALESCE(t3.volume,0)-COALESCE(t4.volume,0)>0
ORDER BY T1.nama_barang
